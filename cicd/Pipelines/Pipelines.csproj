<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
      <AssemblyName>Inpay.DevOps.Pulumi.Pipelines</AssemblyName>
  </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Automatron.AzureDevOps" Version="0.0.0-alpha-11" />
        <PackageReference Include="JetBrains.Annotations" Version="2024.2.0" />
        <PackageReference Include="MinVer" Version="6.0.0">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="SimpleExec" Version="12.0.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\src\PulumiSharp.AzureDevOps.Automatron\PulumiSharp.AzureDevOps.Automatron.csproj" />
    </ItemGroup>

</Project>
